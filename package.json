  },
  "engines": {
    "node": ">=8.1.4",
    "npm": "6.4.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "@babel/runtime-corejs2": "^7.1.5",
    "chalk": "^2.3.0",
    "clui": "^0.3.6",
    "figlet": "^1.2.1",
    "fs-extra": "^7.0.1",
    "inquirer": "^5.0.1",
    "lodash": "^4.17.11",
    "ora": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
